<Window x:Class="ClientForMessenger.AdminConsole"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientForMessenger"
        mc:Ignorable="d"
        Title="AdminConsole" Height="450" Width="499.452" WindowStyle="ToolWindow" Background="Transparent">
    <Grid >
        <Border Background="Black" Opacity="0.6"></Border>
        <StackPanel x:Name="consolePanel">
            <TextBox x:Name="mainconsole" FontSize="15" Background="Transparent" Foreground="LightSkyBlue"  CaretBrush="White" KeyUp="mainconsole_KeyUp"/>
            <TextBlock x:Name="responseBlock" FontSize="15" Background="Transparent" Foreground="LightSkyBlue" TextWrapping="Wrap"/>
        </StackPanel>
    </Grid>
</Window>
